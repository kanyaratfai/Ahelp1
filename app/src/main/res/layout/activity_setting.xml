<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context="com.example.pareeya.ahelp.SettingActivity"
    android:background="@drawable/bgset">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/message"
        android:id="@+id/textView"
        android:textSize="14sp"
        android:background="#e0e0fffe"
        android:textColor="#535353"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="15dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/ok_set"
        android:id="@+id/button14"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:textSize="18sp"
        android:textColor="#ffffff"
        android:background="#1c6ab3"
        android:textStyle="bold"
        android:onClick="clickSetting"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:paddingBottom="10dp"
        android:paddingTop="10dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#e0e0fffe"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="@string/callAuto"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:id="@+id/textView6"
                android:paddingLeft="320dp" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_height="wrap_content"
                android:id="@+id/textView9"
                android:layout_weight="1"
                android:layout_width="180dp" />

            <Button
                android:layout_height="wrap_content"
                android:id="@+id/button2"
                android:layout_weight="1"
                android:background="@drawable/butdel"
                android:layout_width="10dp" />

            <RadioButton
                android:layout_height="wrap_content"
                android:id="@+id/radioButton6"
                android:layout_weight="1"
                android:paddingRight="10dp"
                android:layout_width="0.5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                android:layout_weight="1"
                android:layout_width="180dp" />

            <Button
                android:layout_height="wrap_content"
                android:id="@+id/button3"
                android:layout_weight="1"
                android:background="@drawable/butdel"
                android:layout_width="10dp" />

            <RadioButton
                android:layout_height="wrap_content"
                android:id="@+id/radioButton7"
                android:layout_weight="1"
                android:paddingRight="10dp"
                android:layout_width="0.5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_height="wrap_content"
                android:id="@+id/textView11"
                android:layout_weight="1"
                android:layout_width="180dp" />

            <Button
                android:layout_height="wrap_content"
                android:id="@+id/button4"
                android:layout_weight="1"
                android:background="@drawable/butdel"
                android:layout_width="10dp" />

            <RadioButton
                android:layout_height="wrap_content"
                android:id="@+id/radioButton8"
                android:layout_weight="1"
                android:paddingRight="10dp"
                android:layout_width="0.5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_height="wrap_content"
                android:id="@+id/textView12"
                android:layout_weight="1"
                android:layout_width="180dp" />

            <Button
                android:layout_height="wrap_content"
                android:id="@+id/button5"
                android:layout_weight="1"
                android:background="@drawable/butdel"
                android:layout_width="10dp" />

            <RadioButton
                android:layout_height="wrap_content"
                android:id="@+id/radioButton9"
                android:layout_weight="1"
                android:paddingRight="10dp"
                android:layout_width="0.5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_height="wrap_content"
                android:id="@+id/textView13"
                android:layout_weight="1"
                android:layout_width="180dp" />

            <Button
                android:layout_height="wrap_content"
                android:id="@+id/button6"
                android:layout_weight="1"
                android:background="@drawable/butdel"
                android:layout_width="10dp" />

            <RadioButton
                android:layout_height="wrap_content"
                android:id="@+id/radioButton10"
                android:layout_weight="1"
                android:paddingRight="10dp"
                android:layout_width="0.5dp" />
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>
