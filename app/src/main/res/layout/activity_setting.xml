<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.pareeya.ahelp.SettingActivity"
    android:background="@drawable/st">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/ok_set"
        android:id="@+id/button14"
        android:layout_gravity="bottom"
        android:layout_weight="1"
        android:layout_alignParentBottom="true"
        android:textSize="@dimen/h2"
        android:layout_centerHorizontal="true"
        android:textColor="#ffffff"
        android:background="@color/colorPrimaryDark"
        android:textStyle="bold"
        android:onClick="clickSetting" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignParentTop="false"
        android:id="@+id/linearLayout"
        android:padding="15dp"
        android:background="#e0e0fffe">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText"
                android:layout_weight="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_call"
                android:id="@+id/button2"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/delete_call"
                android:id="@+id/button3"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <EditText
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText2"
                android:layout_weight="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_call"
                android:id="@+id/button4"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/delete_call"
                android:id="@+id/button5"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <EditText
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText3"
                android:layout_weight="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_call"
                android:id="@+id/button7"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/delete_call"
                android:id="@+id/button6"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <EditText
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText4"
                android:layout_weight="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_call"
                android:id="@+id/button9"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/delete_call"
                android:id="@+id/button8"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1">

            <EditText
                android:layout_width="250dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText5"
                android:layout_weight="1" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/add_call"
                android:id="@+id/button11"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/delete_call"
                android:id="@+id/button10"
                android:layout_weight="1"
                android:textSize="@dimen/h4"
                android:textStyle="bold"
                android:textColor="@android:color/background_light" />

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/message"
        android:id="@+id/textView"
        android:textSize="@dimen/h3"
        android:padding="15dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#e0e0fffe"
        android:textColor="#535353" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/call"
        android:id="@+id/textView2"
        android:textSize="@dimen/h3"
        android:layout_below="@+id/linearLayout"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:padding="15dp"
        android:background="#e0e0fffe"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#535353" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/textView2"
        android:background="#e0e0fffe">

        <EditText
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText6"
            android:layout_weight="1" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/add_call"
            android:id="@+id/button12"
            android:layout_weight="1"
            android:textSize="@dimen/h4"
            android:textStyle="bold"
            android:textColor="@android:color/background_light" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/delete_call"
            android:id="@+id/button13"
            android:layout_weight="1"
            android:textSize="@dimen/h4"
            android:textStyle="bold"
            android:textColor="@android:color/background_light" />

    </LinearLayout>

</RelativeLayout>
